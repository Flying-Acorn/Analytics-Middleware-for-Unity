    "dotnet.preferCSharpExtension": true,
    "dotnet.defaultSolution": "Analytics-Middleware-for-Unity.sln",
    "dotnet.server.useOmnisharp": false,
    "dotnet.unitTests.runSettingsPath": "",
    "dotnet.completion.showCompletionItemsFromUnimportedNamespaces": false,
    "unity.enableStickyDecorations": true,
    "unity.defaultBreakpointsBehavior": "everywhere",
    "csharp.semanticHighlighting.enabled": true,
    "csharp.format.enable": true,
    "omnisharp.enableRoslynAnalyzers": false,
    "dotnet.inlayHints.enableInlayHintsForParameters": true,
    "csharp.inlayHints.enableInlayHintsForTypes": true
